class Manager {
  String? name;

  void sayHello(String name) {
    print('Hello $name, my name is ${this.name}');
  }
}

class VicePresident extends Manager {
  void introduce() {
    // Mengakses method parent
    sayHello("Hayato");
  }
}

void main() {
  var manager = Manager();
  manager.name = 'Amirah Zahratul Huda';
  manager.sayHello('Haruka'); 
  // Output: Hello Haruka, my name is Amirah Zahratul Huda

  var vp = VicePresident();
  vp.name = 'Amirah Zahratul Huda';
  vp.sayHello('Hayato'); 
  // Output: Hello Hayato, my name is Amirah Zahratul Huda

  // Contoh memanggil method parent melalui method di subclass
  vp.introduce(); 
  // Output: Hello Hayato, my name is Amirah Zahratul Huda
}
